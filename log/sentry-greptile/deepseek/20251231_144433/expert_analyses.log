ueryset = queryset\n        self.order_by = order_by\n        self.max_date = max_date\n        self.min_date = min_date\n\n    def get_result(\n        self, limit: int = 100, cursor: Cursor | None = None, **kwargs: Any\n    ) -> tuple[Sequence[T], Cursor]:\n        if cursor is None:\n            cursor = Cursor(0, 0, 0)\n\n        # Determine the direction of pagination\n        is_prev = cursor.value < 0\n        value = abs(cursor.value)\n\n        # Build the query\n        qs = self.queryset\n        if self.max_date:\n            qs = qs.filter(datetime__lte=self.max_date)\n        if self.min_date:\n            qs = qs.filter(datetime__gte=self.min_date)\n\n        # Apply cursor filter\n        if cursor.value != 0:\n            if is_prev:\n                qs = qs.filter(datetime__gt=datetime.fromtimestamp(value))\n            else:\n                qs = qs.filter(datetime__lt=datetime.fromtimestamp(value))\n\n        # Order and limit\n        qs = qs.order_by(self.order_by)\n        results = list(qs[:limit])\n\n        # Build next and previous cursors\n        if results:\n            next_value = int(results[-1].datetime.timestamp())\n            prev_value = int(results[0].datetime.timestamp())\n        else:\n            next_value = cursor.value if not is_prev else 0\n            prev_value = cursor.value if is_prev else 0\n\n        next_cursor = Cursor(value=next_value, offset=0, limit=limit)\n        prev_cursor = Cursor(value=-prev_value, offset=0, limit=limit)\n\n        return results, next_cursor if not is_prev else prev_cursor\n\n\nclass OptimizedCursorPaginator(BasePaginator[T]):\n    \"\"\"\n    Optimized cursor paginator that supports negative offsets for efficient reverse pagination.\n    \n    This paginator allows negative offset values to enable efficient backward pagination\n    without requiring a separate query for counting total records.\n    \"\"\"\n\n    def __init__(self, queryset: QuerySet[T], order_by: str = \"-pk\"):\n    